{
  "name": "jonas-mattes",
  "version": "1.0.0",
  "private": true,
  "description": "jonas-mattes",
  "author": "JonasMa",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lighthouse": "lighthouse http://localhost:9000/ --output json --output-path ./lighthouse.report.json",
    "lighthouse:badges": "node lighthouse.badges.js",
    "lighthouse:fresh": "npm run clean && npm run build && concurrently --kill-others \"npm run serve\" \"npm run lighthouse\""
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "gatsby": "^5.13.3",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-postcss": "^6.13.1",
    "gatsby-plugin-react-i18next": "3.0.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-json": "^5.13.1",
    "i18next": "22.5.1",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dynamic-scroll-menu": "^1.0.2",
    "react-i18next": "12.3.1",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "badge-maker": "3.3.1",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.32",
    "prettier": "3.2.5",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "overrides": [
      {
        "files": [
          "**/*.jsonc",
          "**/*.eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  }
}
